{
  "name": "@donskelle/pwa-helpers",
  "version": "0.5.1",
  "description": "git packages test",
  "module": "src/index.js",
  "scripts": {
    "start": "npx http-server dist",
    "build": "rollup -c",
    "size": "size-limit",
    "size-build": "echo 'no build'",
    "lint": "eslint src",
    "lint:fix": "prettier --write src && eslint --fix src",
    "release": "release-it",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Donskelle/pwa-helpers.git"
  },
  "author": "Fabian Puszies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Donskelle/pwa-helpers/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Donskelle/pwa-helpers#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@release-it/conventional-changelog": "^1.1.2",
    "@size-limit/preset-big-lib": "^4.4.5",
    "auto-changelog": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "release-it": "^13.5.4",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-workbox": "^5.0.1",
    "size-limit": "^4.4.5"
  },
  "release-it": {
    "hooks": {
      "before:release": "npx auto-changelog -p && git add CHANGELOG.md"
    },
    "git": {
      "changelog": "npx auto-changelog --stdout --commit-limit false --unreleased --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0"
  }
}
