{
  "name": "@donskelle/pwa-helpers",
  "version": "0.2.0",
  "description": "git packages test",
  "module": "src/index.js",
  "scripts": {
    "lint": "npx eslint src",
    "lint:fix": "npx prettier --write src && npx eslint --fix src",
    "release": "npx release-it",
    "test": "npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Donskelle/pwa-helpers.git"
  },
  "author": "Fabian Puszies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Donskelle/pwa-helpers/issues"
  },
  "homepage": "https://github.com/Donskelle/pwa-helpers#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "prettier": "^2.0.4",
    "release-it": "^13.5.4",
    "service-worker-mock": "^2.0.5"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
